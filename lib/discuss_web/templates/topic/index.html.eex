<h4>Topics</h4>

<ul class="collection">
    <%= for topic <- @topics do %>
        <li class="collectio-item">
            <%= topic.title %>
        </li>
    <% end %>
</ul>

<div class="fixed-action-btn">
    <%= link to: Routes.topic_path(@conn, :new), class: "btn-floating waves-effect waves-light btn-large red" do %>
        <i class="material-icons">add</i>
    <% end %>
</div>
